{
  "version": "1.1.0",
  "name": "fighter-advice",
  "homepage": "https://fighter-advice.barbajoe.tech/",
  "type": "module",
  "keywords": [
    "svelte",
    "sveltekit",
    "typescript",
    "vite",
    "vitest",
    "muaythai",
    "muay-thai",
    "payloadcms",
    "payload-cms",
    "northflank"
  ],
  "scripts": {
    "all": "pnpm clean && pnpm i && pnpm format && pnpm lint && pnpm check && pnpm build && pnpm test",
    "clean": "rm -rf .netlify .svelte-kit build",
    "start": "vite dev",
    "build": "vite build --logLevel=error",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "prettier --write . --log-level warn"
  },
  "devDependencies": {
    "@barbajoe/css-lib": "0.1.8",
    "@barbajoe/svelte-lib": "0.2.6",
    "@sveltejs/adapter-netlify": "2.0.8",
    "@sveltejs/kit": "1.27.7",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "@zerodevx/svelte-toast": "0.9.5",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.35.1",
    "prettier": "3.1.0",
    "prettier-plugin-svelte": "3.1.2",
    "svelte": "4.2.8",
    "svelte-check": "3.6.2",
    "svelte-search": "2.0.1",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "vite": "4.5.1",
    "vitest": "0.34.6"
  }
}
