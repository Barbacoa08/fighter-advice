{
  "version": "1.1.0",
  "name": "fighter-advice",
  "type": "module",
  "keywords": [
    "svelte",
    "sveltekit",
    "typescript",
    "vite",
    "vitest",
    "muaythai",
    "muay-thai",
    "payloadcms",
    "payload-cms",
    "northflank"
  ],
  "scripts": {
    "all": "pnpm clean && pnpm i && pnpm format && pnpm lint && pnpm check && pnpm build && pnpm test",
    "clean": "rm -rf .netlify .svelte-kit build",
    "start": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-netlify": "2.0.7",
    "@sveltejs/kit": "1.18.0",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "@zerodevx/svelte-toast": "0.9.3",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-svelte3": "4.0.0",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "2.10.0",
    "svelte": "3.59.1",
    "svelte-check": "3.3.2",
    "svelte-search": "2.0.1",
    "tslib": "2.5.2",
    "typescript": "5.0.4",
    "vite": "4.3.8",
    "vitest": "0.31.1"
  }
}
